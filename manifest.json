{
  "name": "Canvas Popup Remover",
  "description": "Remove the annoying admin messages in Canvas",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png"
  },
  "content_scripts": [
    {
    "matches": ["*://*.instructure.com/"],
    "js": ["BEGONE.js"],
    "run_at": "document_end"
    }
  ]
}
